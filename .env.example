APP_NAME=Laravel # Application name
APP_ENV=local # Environment name (local, production, etc.)
APP_KEY= # Encryption key generated via `php artisan key:generate`
APP_DEBUG=true # Debug mode for detailed error messages
APP_TIMEZONE=UTC # Default timezone
APP_URL=http://localhost # Base application URL

APP_LOCALE=en # Default application locale
APP_FALLBACK_LOCALE=en # Fallback locale for translations
APP_FAKER_LOCALE=en_US # Locale for Faker data

APP_MAINTENANCE_DRIVER=file # Where to store maintenance mode flag

PHP_CLI_SERVER_WORKERS=4 # Workers for PHP built-in server

BCRYPT_ROUNDS=12 # Cost factor for bcrypt hashing

LOG_CHANNEL=stack # Default log channel
LOG_STACK=single # Channel used for stack logging
LOG_DEPRECATIONS_CHANNEL=null # Where to log deprecation warnings
LOG_LEVEL=debug # Minimum log level

DB_CONNECTION=sqlite # Database driver (sqlite, mysql, etc.)

SESSION_DRIVER=database # Session storage driver
SESSION_LIFETIME=120 # Session lifetime in minutes
SESSION_ENCRYPT=false # Whether to encrypt session data
SESSION_PATH=/ # Session cookie path
SESSION_DOMAIN=null # Session domain

BROADCAST_CONNECTION=pusher # Broadcasting driver
PUSHER_APP_ID= # Pusher application ID
PUSHER_APP_KEY= # Pusher key
PUSHER_APP_SECRET= # Pusher secret
PUSHER_APP_CLUSTER=mt1 # Pusher cluster
VITE_PUSHER_APP_KEY="${PUSHER_APP_KEY}" # Front-end Pusher key
VITE_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}" # Front-end cluster
FILESYSTEM_DISK=local # Default filesystem disk
QUEUE_CONNECTION=database # Queue driver

CACHE_STORE=database # Cache driver
CACHE_PREFIX= # Cache key prefix

MEMCACHED_HOST=127.0.0.1 # Memcached host

REDIS_CLIENT=phpredis # Redis client
REDIS_HOST=127.0.0.1 # Redis host
REDIS_PASSWORD=null # Redis password
REDIS_PORT=6379 # Redis port

MAIL_MAILER=log # Mail driver
MAIL_SCHEME=null # Optional scheme (tls, etc.)
MAIL_HOST=127.0.0.1 # Mail server host
MAIL_PORT=2525 # Mail server port
MAIL_USERNAME=null # Mail username
MAIL_PASSWORD=null # Mail password
MAIL_FROM_ADDRESS="hello@example.com" # Sender address
MAIL_FROM_NAME="${APP_NAME}" # Sender name

AWS_ACCESS_KEY_ID= # AWS access key
AWS_SECRET_ACCESS_KEY= # AWS secret key
AWS_DEFAULT_REGION=us-east-1 # AWS region
AWS_BUCKET= # AWS S3 bucket name
AWS_USE_PATH_STYLE_ENDPOINT=false # Enable path style endpoints

VITE_APP_NAME="${APP_NAME}" # Front-end app name

GOOGLE_MAPS_API_KEY= # Google Maps API key for geocoding

LISTING_CLEANUP_THRESHOLD_DAYS=60 # Days after available_from to clean up listings
LISTING_STATISTICS_CACHE_TTL=86400 # Seconds to cache listing statistics
